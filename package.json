{
  "name": "vue-layout-manager",
  "version": "0.1.4",
  "private": false,
  "main": "dist/vue-layout-manager.common.js",
  "module": "dist/vue-layout-manager.umd.js",
  "unpkg": "dist/vue-layout-manager.umd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-layout-manager --entry ./src/library-entry.ts",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "versionBump:minor": "npm version minor --no-git-tag-version --force",
    "versionBump:patch": "npm version patch --no-git-tag-version --force",
    "npm:pack:inspect": "tar tvf $(npm pack) && rm *.tgz"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "lodash": "^4.17.15",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.153",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-e2e-cypress": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
